# Environment Variables para QA Testing

# API Configuration (REQUERIDO - Cambiar por tus valores reales)
API_BASE_URL=https://tu-api-domain.com
API_TOKEN=tu-token-aqui

# Test Configuration
TEST_ENVIRONMENT=development
TEST_TIMEOUT=30000
TEST_RETRIES=2

# Media API Specific
MEDIA_ENDPOINT=/api/media
DEFAULT_LIMIT=100
DEFAULT_SKIP=0

# Test Data (IDs de ejemplo del endpoint)
TEST_MEDIA_ID=5ee18728ccc0ce5debbebf36
TEST_CATEGORY_ID=5ee18728ccc0ec8debbeb9i4
TEST_JOB_ID=ac34e8ba-38d1-4da5-91e1-c1d39edb6851

# Authentication
X_API_TOKEN_HEADER=X-API-Token

# Logging
LOG_LEVEL=info
ENABLE_DEBUG_LOGS=false

# CI/CD
CI=false
HEADLESS=true

# Test Reports
REPORT_OUTPUT_DIR=test-results
ENABLE_HTML_REPORT=true
ENABLE_JSON_REPORT=true
